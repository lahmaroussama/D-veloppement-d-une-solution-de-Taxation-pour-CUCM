 {% extends 'baseadmin.html.twig' %}

{% block title %}Register{% endblock %}

{% block body %}
 <div class="col-10 grid-margin stretch-card">
              <div class="card">
                <div class="card-body">
                  <h4 class="card-title">Basic form elements</h4>
                  <p class="card-description">
                    Basic form elements
                  </p>
                   {{ form_start(f) }}
              <form class="pt-3">
                <div class="form-group">
               
                  {{ form_row(f.email,{'attr':{'class':'form-control form-control-lg'} }) }}
                </div>
               
       
                <div class="form-group">
                  
                  {{ form_row(f.plainPassword , {'attr':{'class':'form-control form-control-lg'} }) }}
                </div>
               
        
                <div class="form-group">
                  {{ form_row(f.last_name,{'attr':{'class':'form-control form-control-lg'} }) }}
                </div>
                
        

       
                <div class="form-group">
                  {{ form_row(f.first_name,{'attr':{'class':'form-control form-control-lg'} }) }}
                </div>
                <div class="form-group">
                  {{ form_row(f.image,{'attr':{'class':'form-control form-control-lg'} }) }}
                </div>
                <div class="form-group">
                  {{ form_row(f.roles,{'attr':{'class':'form-control '} }) }}
                </div>
                <div class="form-group">
                  {{ form_row(f.numero,{'attr':{'class':'form-control form-control-lg'} }) }}
                </div>

                 <button type="submit" class="btn btn-block btn-primary btn-lg font-weight-medium auth-form-btn">Register</button>
    
               
                {{ form_end(f) }}
                <div class="my-2 d-flex justify-content-between align-items-center">
                  <div class="form-check">
                    
                  </div>
                  
                </div>
                
                
              </form>
                </div>
              </div>
            </div>
            {% endblock %}